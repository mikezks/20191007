{
  "name": "starter",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "start /b npm run serve & start /b npm run api",
    "start-unix": "npm run serve & npm run api",
    "serve": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e --port 4299",
    "tsc": "tsc",
    "api": "json-server --watch ./projects/api/db.json",
